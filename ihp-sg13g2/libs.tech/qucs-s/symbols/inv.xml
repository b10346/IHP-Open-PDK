<?xml version="1.0"?>
<Component
  xsi:noNamespaceSchemaLocation="Component.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  library="IHP SG13G2 Standard Cells" names="sg13g2_inv_1, sg13g2_inv_2, sg13g2_inv_4, sg13g2_inv_8" schematic_id="INV">
    <Description>
        Inverter
    </Description>
    <Models>
        <DefaultModel value="xinv">
        </DefaultModel>
        <SpiceModel value="xinv">
        </SpiceModel>
    </Models>
    <Netlists>
        <NgspiceNetlist value=
            "{{{Letter}}}::schematic_id='nequal'INV{PartCounter} {nets} {VDD} {VSS} {{{model}}}::model='nonempty'">
        </NgspiceNetlist>
        <CDLNetlist value="{NgspiceNetlist}"></CDLNetlist>
    </Netlists>
    <Symbols>
        <Symbol id="Standard">
           <File>{QUCS_S_COMPONENTS_LIBRARY}/inv.sym</File>
        </Symbol>
    </Symbols>
    <Parameters>
        <Parameter name="Letter" unit="" default_value="X" show="false">
            <Description>Spice prefix</Description>
        </Parameter>
        <Parameter name="model" unit="" default_value="{names}" show="false">
            <Description>[sg13g2_inv_1,sg13g2_inv_2,sg13g2_inv_4,sg13g2_inv_8]</Description>
        </Parameter>
        <Parameter name="VDD" unit="" default_value="VDD" show="false">
            <Description>VDD terminal net name</Description>
        </Parameter>
        <Parameter name="VSS" unit="" default_value="VSS" show="false">
            <Description>VSS terminal net name</Description>
        </Parameter>
    </Parameters>
</Component>

